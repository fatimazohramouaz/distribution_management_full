      <field name="name">Distribution Sale Order</field>
      <field name="prefix">SO-DIST/</field>
      <field name="padding">4</field>
      <field name="implementation">no_gap</field>
    </record>
    <record id="seq_po_distribution" model="ir.sequence">
      <field name="name">Distribution Purchase Order</field>
